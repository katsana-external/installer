{
    "name": "orchestra/installer",
    "description": "Orchestra Platform Installer",
    "homepage": "http://orchestraplatform.com/docs/latest/",
    "license": "MIT",
    "keywords": ["orchestra-platform", "orchestral", "installer"],
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Installation\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Installation\\Tests\\" : "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "orchestra/contracts": "~3.6.0"
    },
    "require-dev": {
        "orchestra/foundation": "~3.6.0",
        "orchestra/testing": "~3.6.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.6-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
